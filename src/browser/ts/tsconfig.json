{
    "compilerOptions": {
      "module": "ES2022",
      "lib": [
        "ES5",
        "ES2015",
        "DOM",
        "ScriptHost"],
      "outDir": "../../../content",
      "rootDir": "./",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "bundler",
    },
    "include": ["./**/*.ts"],
    "exclude": ["node_modules"]
  }
